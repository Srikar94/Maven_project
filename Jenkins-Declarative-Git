pipeline{
  agent any
  environment{
    PATH = "/opt/maven/bin:$PATH"
  }
  stages{
    stage("Git Checkout"){
      steps{
        git credentialsId: 'GitHub', url: 'https://github.com/Srikar94/Maven_project.git'
      }
    }
    stage("Maven Build"){
      steps{
        sh "mvn clean package"
      }
    }
  }
}
